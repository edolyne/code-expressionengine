{
    "name": "ee",
    "displayName": "ExpressionEngine",
    "description": "ExpressionEngine syntax highlighting",
    "version": "0.1.0",
    "publisher": "EdolyneLong",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "extensionDependencies": [
        "html","xml"
    ],
    "files.associations": {
        "*.html": "html",
        "*.feed": "xml"
    },
    "icon": "icon.svg",
    "contributes": {
        "languages": [{
            "id": "html",
            "aliases": ["ExpressionEngine", "ee", "html", "xml"],
            "extensions": [".html"],
            "mimetypes": ["text/html", "text/x-jshtm", "text/template", "text/ng-template", "application/xhtml+xml"],
            "configuration": "./ee.configuration.json"
        },{
            "id": "xml",
            "aliases": ["ExpressionEngine", "ee", "html", "xml"],
            "extensions": [".feed"],
            "mimetypes": ["text/html", "text/x-jshtm", "text/template", "text/ng-template", "application/xhtml+xml"],
            "configuration": "./ee.configuration.json"
        }],
        "grammars": [{
            "language": "html",
            "scopeName": "text.html.ee",
            "path": "./syntaxes/ee.tmLanguage"
        },{
            "language": "xml",
            "scopeName": "text.html.ee",
            "path": "./syntaxes/ee.tmLanguage"
        }]
    }
}