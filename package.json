{
    "name": "ee",
    "displayName": "ExpressionEngine",
    "description": "ExpressionEngine syntax highlighting",
    "version": "0.1.0",
    "publisher": "EdolyneLong",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "extensionDependencies": [
        "html",
        "xml"
    ],
    "icon": "icon.svg",
    "contributes": {
        "languages": [{
            "id": "ee",
            "aliases": [
                "HTML (ExpressionEngine)",
                "ee"
            ],
            "extensions": [
                ".html",
                ".feed"
            ],
            "mimetypes": [
                "text/html",
                "text/x-jshtm",
                "text/template",
                "text/ng-template",
                "application/xhtml+xml"
            ],
            "configuration": "./ee.configuration.json"
        }],
        "grammars": [{
            "language": "ee",
            "scopeName": "text.html.ee",
            "path": "./syntaxes/ee.tmLanguage"
        }]
    }
}